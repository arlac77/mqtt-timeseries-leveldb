{
  "name": "mqtt-timeseries-leveldb",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "npm@11.10.0",
  "exports": {
    ".": "./src/worker.mjs"
  },
  "description": "writes mqtt timeseries messages into leveldb",
  "keywords": [
    "mqtt",
    "timeseries"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "0BSD",
  "scripts": {
    "test": "node --run test:ava",
    "test:ava": "ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src**/*.mjs",
    "lint": "node --run lint:docs",
    "lint:docs": "documentation lint ./src**/*.mjs"
  },
  "dependencies": {
    "mqtt": "^5.15.0"
  },
  "devDependencies": {
    "ava": "^7.0.0",
    "c8": "^11.0.0",
    "documentation": "^14.0.3",
    "leveldown": "^6.1.1",
    "levelup": "^5.1.1",
    "semantic-release": "^25.0.3"
  },
  "engines": {
    "node": ">=14.19.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/mqtt-timeseries-leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/mqtt-timeseries-leveldb#readme",
  "template": {
    "files": [
      {
        "merger": "Readme",
        "options": {
          "badges": {
            "Maintainability": {
              "id": "ffa8cca9d6160319798f"
            }
          }
        }
      }
    ],
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-javascript-component"
    ]
  }
}
