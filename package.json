{
  "name": "mqtt-timeseries-leveldb",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/worker.mjs"
  },
  "description": "writes mqtt timeseries messages into leveldb",
  "keywords": [
    "mqtt",
    "timeseries"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "mqtt": "^4.2.1"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "c8": "^7.7.2",
    "documentation": "^13.2.5",
    "leveldown": "^6.0.0",
    "levelup": "^5.0.0",
    "semantic-release": "^17.4.3"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/mqtt-timeseries-leveldb#readme",
  "template": {
    "files": [
      {
        "merger": "Readme",
        "options": {
          "badges": {
            "Maintainability": {
              "id": "ffa8cca9d6160319798f"
            }
          }
        }
      }
    ],
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
