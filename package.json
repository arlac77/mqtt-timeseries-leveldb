{
  "name": "mqtt-timeseries-leveldb",
  "description": "writes mqtt timeseries messages into leveldb",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/mqtt-timeseries-leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/mqtt-timeseries-leveldb#readme",
  "scripts": {
    "cover": "nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.1",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.3.0",
    "rollup": "^0.51.2",
    "semantic-release": "^9.1.0",
    "xo": "^0.19.0"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "version": "0.0.0-semantic-release",
  "main": "dist/worker.js",
  "module": "src/worker.js",
  "keywords": [
    "mqtt",
    "timeseries"
  ],
  "dependencies": {
    "leveldown": "^1.9.0",
    "levelup": "^1.3.9",
    "mqtt": "^2.13.1"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      null
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    },
    "badges": {
      "Maintainability": {
        "id": "ffa8cca9d6160319798f"
      }
    }
  }
}
