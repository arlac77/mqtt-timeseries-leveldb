{
  "name": "mqtt-timeseries-leveldb",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/worker.js",
  "module": "src/worker.js",
  "description": "writes mqtt timeseries messages into leveldb",
  "keywords": [
    "mqtt",
    "timeseries"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/worker.js --section=API",
    "semantic-release": "semantic-release",
    "lint": "documentation lint src/worker.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "leveldown": "^4.0.0",
    "mqtt": "^2.18.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^8.0.0",
    "levelup": "^3.1.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^12.0.2",
    "rollup": "^0.61.1",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^15.6.0",
    "travis-deploy-once": "^5.0.0"
  },
  "engines": {
    "node": ">=10.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/mqtt-timeseries-leveldb#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    },
    "files": [
      {
        "merger": "Readme",
        "options": {
          "badges": {
            "Maintainability": {
              "id": "ffa8cca9d6160319798f"
            }
          }
        }
      }
    ]
  }
}
