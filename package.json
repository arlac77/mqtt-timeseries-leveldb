{
  "name": "mqtt-timeseries-leveldb",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/worker.mjs",
  "module": "src/worker.mjs",
  "description": "writes mqtt timeseries messages into leveldb",
  "keywords": [
    "mqtt",
    "timeseries"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*.mjs' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/worker.mjs --section=API",
    "lint": "documentation lint src/worker.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {
    "mqtt": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^2.3.0",
    "c8": "^5.0.4",
    "documentation": "^12.1.2",
    "esm": "^3.2.25",
    "leveldown": "^5.1.1",
    "levelup": "^4.1.0",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.24"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/mqtt-timeseries-leveldb#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    },
    "files": [
      {
        "merger": "Readme",
        "options": {
          "badges": {
            "Maintainability": {
              "id": "ffa8cca9d6160319798f"
            }
          }
        }
      }
    ]
  }
}
