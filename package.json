{
  "name": "mqtt-timeseries-leveldb",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/worker.mjs",
  "exports": {
    ".": "./src/worker.mjs"
  },
  "description": "writes mqtt timeseries messages into leveldb",
  "keywords": [
    "mqtt",
    "timeseries"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/worker.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/worker.mjs"
  },
  "dependencies": {
    "mqtt": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.1",
    "documentation": "^13.0.2",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
    "semantic-release": "^17.1.2"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mqtt-timeseries-leveldb/issues"
  },
  "homepage": "https://github.com/arlac77/mqtt-timeseries-leveldb#readme",
  "template": {
    "files": [
      {
        "merger": "Readme",
        "options": {
          "badges": {
            "Maintainability": {
              "id": "ffa8cca9d6160319798f"
            }
          }
        }
      }
    ],
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
